#include <stdio.h>
int primo(int n){
  int divisores=0;
  int i;

  for(i=0; i<=n;i++){
  }
  if(n % i == 0){
    divisores++;
  }
  if (divisores != 2){
    return 1;
  }
  
  else{
    return 0;
  }
}


int main(void) {
  int numero;

  printf("digite um numero:\n");
  scanf("%d", &numero);

  if (primo(numero)==1)
    printf("o numero %d é primo\n", numero);
  
  else 
  printf("o numero %d não é primo\n", numero);
  
  return 0;
}
